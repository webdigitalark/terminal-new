{#
/**
 * @file
 * Template for a 2 column and bottom row layout.
 *
#}
<div {{ attributes.setAttribute('id', settings.css_id).addClass('row expanded', 'terminal-2col', settings.css_classes) }}>
  <div class="small-12 large-5 columns">
    {{ content.left }}
  </div>
  <div class="small-12 large-7 columns">
    {{ content.right }}
  </div>
  <div class="small-12 columns">
    {{ content.bottom }}
  </div>
</div>
